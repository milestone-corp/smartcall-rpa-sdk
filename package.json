{
  "name": "@smartcall/rpa-sdk",
  "version": "1.1.0",
  "description": "SmartCall RPA SDK - Redis/BullMQ/Playwright utilities for RPA API developers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./rpa": {
      "import": "./dist/rpa/index.js",
      "types": "./dist/rpa/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "keywords": [
    "smartcall",
    "rpa",
    "redis",
    "bullmq",
    "playwright",
    "queue",
    "automation"
  ],
  "author": "SmartCall Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milestone-corp/smartcall-rpa-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/milestone-corp/smartcall-rpa-sdk/issues"
  },
  "homepage": "https://github.com/milestone-corp/smartcall-rpa-sdk#readme",
  "dependencies": {
    "bullmq": "^5.65.1",
    "ioredis": "^5.8.2"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "playwright": "^1.49.1",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "bullmq": ">=5.0.0",
    "ioredis": ">=5.0.0",
    "playwright": ">=1.40.0"
  },
  "peerDependenciesMeta": {
    "bullmq": {
      "optional": true
    },
    "ioredis": {
      "optional": true
    },
    "playwright": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
